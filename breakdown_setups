 addUserKnob {41 format l "wipe format" -STARTLINE T Reformat1.format}
 addUserKnob {41 operation T Merge1.operation}
 addUserKnob {3 Retime1_output.first l "wipe start"}
 Retime1_output.first {{Wipes_Controller.bd_start}}
 addUserKnob {3 lengthofwipe l "wipe length" -STARTLINE}
 lengthofwipe {{parent.Wipes_Controller.WipeLength}}
