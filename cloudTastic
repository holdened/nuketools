 temp_name1 dist
 temp_expr1 sqrt(pow2(x(pos)-x(posA))+pow2(y(pos)-y(posA))+pow2(y(pos)-y(posA))+pow2(z(pos)-z(posA)))
 temp_name2 distPlus
 temp_expr2 "dist-\[value pos1radius]<=0?0:dist-\[value pos1radius]"
 temp_name3 result
 temp_expr3 "(\[value pos1radius]/distPlus)>1?1:(\[value pos1radius]/distPlus)"
