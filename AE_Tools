   temp_name1 vy
   temp_expr1 "(lightPos.y-Pos.y)/sqrt( pow2(lightPos.x-Pos.x) + pow2(lightPos.y-Pos.y) + pow2(lightPos.z-Pos.z))"
   temp_name2 vz
   temp_expr2 "(lightPos.z-Pos.z)/sqrt( pow2(lightPos.x-Pos.x) + pow2(lightPos.y-Pos.y) + pow2(lightPos.z-Pos.z))"
